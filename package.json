{
  "name": "modules-cdn-webpack-plugin",
  "version": "2.1.0",
  "description": "Get your dependencies from a cdn rather than bundling them in your app",
  "license": "MIT",
  "repository": "mastilver/modules-cdn-webpack-plugin",
  "author": {
    "name": "Thomas Sileghem",
    "email": "th.sileghem@gmail.com",
    "url": "mastilver.com"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "xo && ava",
    "prepublish": "babel src --out-dir lib"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "keywords": [
    "webpack",
    "cdn",
    "unpkg.com",
    "html-webpack-plugin"
  ],
  "dependencies": {
    "find-up": "^2.1.0",
    "parent-module": "^0.1.0",
    "read-pkg": "^2.0.0"
  },
  "peerDependencies": {
    "module-to-cdn": "2.x"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.3.3",
    "html-webpack-plugin": "^2.28.0",
    "module-to-cdn": "^2.0.0",
    "mz": "^2.6.0",
    "rimraf": "^2.6.1",
    "webpack": "^2.6.1",
    "webpack-manifest-plugin": "^1.1.0",
    "xo": "^0.17.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          }
        }
      ]
    ]
  },
  "xo": {
    "esnext": true,
    "space": 4,
    "rules": {
      "eqeqeq": "off",
      "no-eq-null": "off"
    }
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-polyfill",
      "babel-core/register"
    ]
  }
}
